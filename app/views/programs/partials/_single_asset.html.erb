<video id="" class="video-js vjs-default-skin"
  controls preload="auto" width="60%" height="350"
  poster="http://video-js.zencoder.com/oceans-clip.png"
  data-setup='{"example_option":true}'>
 <source src="http://video-js.zencoder.com/oceans-clip.mp4" type='video/mp4' />
</video>
<div id="transcript-box">
  <h2>Transcript</h2>
  <div class="datastream-actions">
    <%#= link_to "Print", print_catalog_path(:id => document.id), :class => "print action", :target => "_blank" %>
    <form action="#" method="GET" class="q_field datastream-action-search">
      <%= text_field_tag :q, nil, :class => "q", :placeholder => "Search Transcript" %>
      <%= image_submit_tag("/assets/magnifying-glass.gif", :alt => "Search Transcript", :class => 'submit') %>
      
    </form>
    <div class="datastream secondary-datastream datastream-transcript datastream-transcript-tei-xml">
      #This will the be transcript data
      <%#= render_tei_transcript(datastream_url(document.fedora_object.datastream["Transcript.tei.xml"], :private => true)) %>
    </div>
  </div>
</div>